{
  "expo": {
    "name": "tubigMobileProject",
    "slug": "tubigMobileProject",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo_dic.png",
      "resizeMode": "contain",
      "backgroundColor": "#F8E2CF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist":{
        "NSLocationWhenInUseUsageDescription": "This app needs your location to function properly.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs your location to function properly even when running in the background."
        
      }

      
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION"
      ]
     
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}